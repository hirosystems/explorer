{
  "name": "@blockstack/explorer",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "NODE_ENV=production next start",
    "lint": "yarn run lint:eslint && yarn run lint:prettier",
    "lint:eslint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --ext .ts,.tsx ./src/ -f unix --fix && prettier --write src/**/*.{ts,tsx} *.js",
    "lint:prettier": "prettier --check src/**/*.{ts,tsx} *.js"
  },
  "license": "MIT",
  "prettier": "@blockstack/prettier-config",
  "dependencies": {
    "@blockstack/ui": "^1.2.0-beta.2",
    "@reduxjs/toolkit": "^1.3.0",
    "dayjs": "^1.8.23",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.3.5",
    "next-redux-wrapper": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux-thunk": "^2.3.0",
    "store": "^2.0.12",
    "styled-components": "^5.0.1",
    "timeago.js": "^4.0.2",
    "use-events": "^1.4.1"
  },
  "devDependencies": {
    "@blockstack/eslint-config": "^1.0.3",
    "@blockstack/prettier-config": "^0.0.6",
    "@blockstack/stacks-blockchain-sidecar-types": "^0.0.6",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/express": "^4.17.3",
    "@types/next": "^9.0.0",
    "@types/node": "^13.9.3",
    "@types/react": "^16.9.25",
    "@types/react-redux": "^7.1.7",
    "@types/redux-thunk": "^2.1.0",
    "@types/store": "^2.0.2",
    "commitlint": "^8.3.5",
    "eslint": "^6.7.2",
    "husky": "^4.2.3",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
